<div id="<%= dom_id compra %>">
  <p>
    <strong>Cliente:</strong>
   <%= compra.cliente.nome if compra.cliente.present? %>
  </p>

  <p>
    <strong>Data:</strong>
    <%= compra.data %>
  </p>

  <p>
    <strong>Descricao:</strong>
    <%= compra.descricao %>
  </p>

  <p>
    <strong>Valor:</strong>
    <%= compra.valor %>
  </p>

</div>
